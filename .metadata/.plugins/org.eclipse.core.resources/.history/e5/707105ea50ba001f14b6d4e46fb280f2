package com.AppArch.Project.Service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service
public class OfferRepoServiceImpl implements OfferRepoService{
    @Autowired
    private OfferRepoService offerRepo;
    
    public Optional<Offer> getOffer(String email, int taskId) {
        OfferKey key = new Offerkey(email, taskId);
        return taskRepository.findById(key);
    }

}
